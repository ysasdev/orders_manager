CREATE TABLE public.orders01
(
    order_id_magento integer NOT NULL,
    product_sku bigint[] NOT NULL,
    salesman_id integer NOT NULL,
    customer_phone_number bigint NOT NULL,
    transaction_mode paymentmode NOT NULL,
    transaction_id character varying(70) NOT NULL,
    transaction_date date NOT NULL,
    xpose_bill_id bigint,
    xpose_bill_status integer NOT NULL,
    billing_name character varying(70) NOT NULL,
    billing_address_field_1 character varying(50),
    billing_address_field_2 character varying(50),
    billing_address_field_3 character varying(50),
    billing_city character varying(20) NOT NULL,
    billing_pin_code character varying(30) NOT NULL,
    billing_state character varying(30) NOT NULL,
    billing_country character varying(20) NOT NULL,
    billing_date date,
    shipment_id bigint NOT NULL,
    shipment_status integer NOT NULL,
    PRIMARY KEY (order_id_magento)
)
WITH (
    OIDS = FALSE
);

ALTER TABLE public.orders01
    OWNER to kdev;
